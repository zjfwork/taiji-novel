.category-wrapper{

}
.category-wrapper .categ-header{
    width: 100%;
    background: white;
    font-size: 14px;
    margin-top: 10px;
}
.categ-header .header-top{
    text-indent: 10px;
    font-size: 16px;
    margin-top: 10px;
    padding: 10px 0;
    font-weight: bold;
}
.categ-header .header-bottom{
    
}
.header-bottom .b-ul{
    width: 100%;
    border-bottom:solid 1px #ccc;
    float: left;
    background: white;
    text-align: center;
}
.b-ul .b-li{
    float: left;
    width: 25%;
    height: 40px;
    box-sizing: border-box;
}
.b-li .b-a{
    display: block;
    margin-top: 10px;
    border-right: solid 1px #f0f0f0;
}